    <bean id="insuranceCompanyEmailService"
          class="de.alectogmbh.friendsurance.product.insurance_company.test.DummyInsuranceCompanyEmailService"/>

    <bean id="friendsuranceResources"
          class="de.alectogmbh.friendsurance.resource.cdn.test.FriendsuranceResourcesDummyImpl"/>

    <bean id="externalContactsProviderFacade"
